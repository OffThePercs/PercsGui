-- Scripts:

local function DICHDPI_fake_script() -- DaHood.LocalScript 
	local script = Instance.new('LocalScript', DaHood)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.DaHoodFrame.Visible = true
		script.Parent.Parent.MM2Frame.Visible = false	
		script.Parent.Parent.HomeFrame.Visible = false
		script.Parent.Parent.AdminFrame.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(DICHDPI_fake_script)()
local function ZIGRN_fake_script() -- MM2.LocalScript 
	local script = Instance.new('LocalScript', MM2)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.DaHoodFrame.Visible = false
		script.Parent.Parent.MM2Frame.Visible = true	
		script.Parent.Parent.HomeFrame.Visible = false
		script.Parent.Parent.AdminFrame.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(ZIGRN_fake_script)()
local function RAWIJ_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.DaHoodFrame.Visible = false
		script.Parent.Parent.MM2Frame.Visible = false	
		script.Parent.Parent.HomeFrame.Visible = true
		script.Parent.Parent.AdminFrame.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(RAWIJ_fake_script)()
local function LRUPZ_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	local FrameObject = script.Parent.FrameObject
	local Open = false
	
	local PostionClosed = UDim2.new (0.46, 01, 0)
	local PositionOpen = UDim2.new(0.46, 0,0.281, 0)
	
	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:connect(function(keyCode)
		if keyCode.keyCode == Enum.KeyCode.h then
			if Open then
				FrameObject:TweenPosition(PostionClosed)
				Open = false
			else
				Open = true
				FrameObject:TweenPosition(PositionOpen)
			end
		end
	end)
	
end
coroutine.wrap(LRUPZ_fake_script)()
local function MEXHA_fake_script() -- Admin.LocalScript 
	local script = Instance.new('LocalScript', Admin)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.DaHoodFrame.Visible = false
		script.Parent.Parent.MM2Frame.Visible = false	
		script.Parent.Parent.HomeFrame.Visible = false
		script.Parent.Parent.AdminFrame.Visible = true
		wait(0.1)
	end)
end
coroutine.wrap(MEXHA_fake_script)()
