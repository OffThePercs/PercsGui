UICorner_2.Parent = percstitle

DaHood.Name = "DaHood"
DaHood.Parent = MainFrame
DaHood.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
DaHood.Position = UDim2.new(0, 54, 0, 273)
DaHood.Size = UDim2.new(0, 200, 0, 50)
DaHood.Font = Enum.Font.SciFi
DaHood.Text = "DaHood"
DaHood.TextColor3 = Color3.fromRGB(0, 0, 0)
DaHood.TextScaled = true
DaHood.TextSize = 14.000
DaHood.TextWrapped = true
DaHood.MouseButton1Down:connect(function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/compwnter/destiny/main/loader'))();
end)

MM2.Name = "MM2"
MM2.Parent = MainFrame
